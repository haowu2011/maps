<h1>Editing Location #<%= @location.id %></h1>

<form action="/locations/<%= @location.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @location.name  %>" type="text" />
  </div>
  <div>
    <label for="address">address</label><br />
    <input id="address" name="address" value="<%= @location.address  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


